{
  "name": "lookup-server",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "express": "4.15.3"
  },
  "scripts": {
    "start": "concurrently 'npm run server' 'npm run client' 'npm run micros'",
    "server": "nodemon server/bin/www",
    "client": "nodemon start-client.js",
    "micros": "concurrently 'npm run user-service' 'npm run gist-service' 'npm run markdown-service' 'npm run qux-service' 'npm run git-socket-service'",
    "user-service": "babel-node ../Micros/user-service/bin/www",
    "gist-service": "babel-node ../Micros/gist-service/bin/www",
    "markdown-service": "babel-node ../Micros/markdown-service/bin/www",
    "qux-service": "babel-node ../Micros/qux-service/bin/www",
    "git-socket-service": "babel-node ../Micros/git-socket-service/bin/www",
    "lint": "eslint ."
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "concurrently": "3.4.0",
    "eslint": "3.19.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "6.9.0"
  }
}
